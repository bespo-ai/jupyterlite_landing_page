{
  "@jupyterlab/apputils-extension:themes": {
    "theme": "JupyterLab Night",
    "theme-scrollbars": true
  },
  "shortcuts": [
    {
      "command": "notebook:run-cell",
      "keys": ["Enter"],
      "selector": ".jp-Notebook:focus"
    },
    {
      "command": "notebook:run-cell-and-select-next",
      "keys": ["Enter"],
      "selector": ".jp-Notebook:focus"
    }
  ]
}
